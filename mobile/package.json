{
  "name": "fortis-mobile",
  "version": "1.0.0",
  "description": "FORTIS Mobile App - Sistema de Votação Eletrônica Seguro",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace FortisMobile.xcworkspace -scheme FortisMobile -configuration Release -destination generic/platform=iOS -archivePath FortisMobile.xcarchive archive",
    "clean": "react-native clean",
    "postinstall": "cd ios && pod install"
  },
  "dependencies": {
    "react": "18.2.0",
